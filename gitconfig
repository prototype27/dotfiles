    cp = cherry-pick
    st = status
    cl = clone
    co = checkout
    br = branch 
[color]
    ui = true
[diff]
    renames = copies
[branch]
    autosetupmerge = true
[push]
    default = tracking
[credential]
    helper = osxkeychain
[core]
    excludesfile = ~/.gitignore

